{
    "manifest_version" : 2,
    "name" : "Missions Extension Template",
    "description" : "A template for creating your own extensions for the Ukaton Missions!",
    "version" : "1.0",
    "permissions" : [
        "activeTab"
    ],
    "content_security_policy" : "object-src 'self'; script-src 'self'; connect-src 'self'",
    "content_scripts" : [
        {
            "matches" : ["<all_urls>"],
            "js" : ["content.js"]
        }
    ],
    "web_accessible_resources" : ["/injection.js", "/Missions.js"],
    "background" : {
        "scripts" : ["background.js"]
    },
    "browser_action" : {
        "default_icon" : "icon.png",
        "default_popup" : "popup.html"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
       "128": "icon.png"
    }
}